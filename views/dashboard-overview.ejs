<body data-bs-spy="scroll" data-bs-target=".navbar" data-bs-offset="70">
    

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->

    
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!-- Header -->
        <%- include('../partials/dashboard-header.ejs') %>

        <!-- SIDEBAR -->
        <%- include('../partials/dashboard-sidebar.ejs') %>

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">

            <div class="container-fluid mt-3">
                <div class="row">
                    <!-- SECTION API Request Counter -->
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-1">
                            <div class="card-body">
                                <h3 class="card-title text-white">API Requests</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white">449</h2>
                                    <!-- <p class="text-white mb-0">January 1st, 2020</p> -->
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-cloud-upload"></i></span>
                            </div>
                        </div>
                    </div>
                    <!-- SECTION Fraud detected counter -->
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-2">
                            <div class="card-body">
                                <h3 class="card-title text-white">High</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white">102</h2>
                                    <!-- <p class="text-white mb-0">January 1st, 2022</p> -->
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-exclamation-triangle"></i></span>
                            </div>
                        </div>
                    </div>
                    <!-- SECTION Average response time  -->
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-3">
                            <div class="card-body">
                                <h3 class="card-title text-white">Response Time</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white">3m</h2>
                                    <!-- <p class="text-white mb-0">Jan - March 2019</p> -->
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-clock-o"></i></span>
                            </div>
                        </div>
                    </div>
                    <!-- SECTION Percentage Accurate -->
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-4">
                            <div class="card-body">
                                <h3 class="card-title text-white">Accuracy</h3>
